# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://your-foundry.openai.azure.com

# Azure Anthropic (Claude) Configuration
AZURE_ANTHROPIC_ENDPOINT=https://your-foundry.openai.azure.com

# Azure DeepSeek Configuration
AZURE_DEEPSEEK_ENDPOINT=https://your-foundry.openai.azure.com

# Google Gemini Configuration
GOOGLE_API_KEY=your-google-api-key

# xAI Grok Configuration
XAI_API_KEY=your-xai-api-key

# Azure Storage (for audit trail)
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your-account;AccountKey=your-key;EndpointSuffix=core.windows.net

# Committee Configuration (optional, defaults provided)
COMMITTEE_PROVIDER_COUNT=3
COMMITTEE_CONSENSUS_THRESHOLD=0.66
COMMITTEE_CONFIDENCE_THRESHOLD=0.75
COMMITTEE_TIMEOUT_MS=30000
COMMITTEE_MIN_SUCCESSFUL_PROVIDERS=2
